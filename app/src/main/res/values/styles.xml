<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextPrice">
        <item name="android:textColor">@color/md_theme_light_primary</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">@font/numero</item>
    </style>

    <style name="TextBlackDefault">
        <item name="android:textColor">@color/md_theme_dark_onTertiary</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/numero</item>
